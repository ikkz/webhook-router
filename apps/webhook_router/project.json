{
  "name": "webhook_router",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/webhook_router/src",
  "targets": {
    "build": {
      "cache": true,
      "executor": "@monodon/rust:build",
      "outputs": [
        "{options.target-dir}"
      ],
      "options": {
        "target-dir": "dist/target/webhook_router"
      },
      "configurations": {
        "production": {
          "release": true
        }
      }
    },
    "test": {
      "cache": true,
      "executor": "@monodon/rust:test",
      "outputs": [
        "{options.target-dir}"
      ],
      "options": {
        "target-dir": "dist/target/webhook_router"
      },
      "configurations": {
        "production": {
          "release": true
        }
      }
    },
    "lint": {
      "cache": true,
      "executor": "@monodon/rust:lint",
      "outputs": [
        "{options.target-dir}"
      ],
      "options": {
        "target-dir": "dist/target/webhook_router"
      }
    },
    "run": {
      "executor": "@monodon/rust:run",
      "outputs": [
        "{options.target-dir}"
      ],
      "options": {
        "target-dir": "dist/target/webhook_router",
        "args": [
          "--username=test",
          "--password=test",
          "--db-path=:memory:"
        ]
      },
      "configurations": {
        "production": {
          "release": true
        }
      }
    },
    "generate-openapi": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/webhook_router",
        "command": "cargo run -- --username=placeholder --password=placeholder --generate-openapi > ../../packages/api-client/openapi.json"
      }
    }
  },
  "tags": []
}