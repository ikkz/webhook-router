{
  "name": "webhook_router",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/webhook_router/src",
  "targets": {
    "build": {
      "cache": true,
      "dependsOn": [
        {
          "projects": [
            "console"
          ],
          "target": "build"
        }
      ],
      "executor": "@monodon/rust:build",
      "outputs": [
        "{options.target-dir}"
      ],
      "options": {
        "target-dir": "dist/target/webhook_router"
      },
      "configurations": {
        "test": {
          "features": [
            "console"
          ]
        },
        "production": {
          "release": true,
          "features": [
            "console"
          ]
        }
      },
      "defaultConfiguration": "test"
    },
    "test": {
      "cache": true,
      "executor": "@monodon/rust:test",
      "outputs": [
        "{options.target-dir}"
      ],
      "options": {
        "target-dir": "dist/target/webhook_router"
      },
      "configurations": {
        "production": {
          "release": true
        }
      }
    },
    "lint": {
      "cache": true,
      "executor": "@monodon/rust:lint",
      "outputs": [
        "{options.target-dir}"
      ],
      "options": {
        "target-dir": "dist/target/webhook_router"
      }
    },
    "run": {
      "executor": "@monodon/rust:run",
      "continuous": true,
      "dependsOn": [
        {
          "projects": [
            "console"
          ],
          "target": "build"
        }
      ],
      "outputs": [
        "{options.target-dir}"
      ],
      "options": {
        "target-dir": "dist/target/webhook_router",
        "args": [
          "--bind=127.0.0.1:3100",
          "--username=admin",
          "--password=admin",
          "--db-path=:memory:"
        ],
        "features": [
          "console"
        ]
      },
      "configurations": {
        "debug": {
          "args": [
            "--username=admin",
            "--password=admin"
          ]
        }
      }
    },
    "generate-openapi": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/webhook_router",
        "command": "cargo run -- --username=admin --password=admin --generate-openapi > ../../packages/api-client/openapi.json"
      }
    }
  },
  "tags": []
}